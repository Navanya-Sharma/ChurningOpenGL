#shader vertex#version 330 corelayout (location = 0) in vec4 position;layout (location = 1) in vec2 texCord;out vec2 v_TexCord;void main(){	gl_Position = position;	v_TexCord = texCord;};#shader fragment#version 330 corelayout(location=0) out vec4 color;in vec2 v_TexCord;uniform vec4 n_color;uniform sampler2D u_Texture;void main(){	vec4 texColor= texture(u_Texture, v_TexCord);	color=texColor;};